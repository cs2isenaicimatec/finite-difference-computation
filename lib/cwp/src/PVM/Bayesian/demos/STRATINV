time    stratInv			\
	model=$model			\
	data=$data			\
	scale=$scale			\
	corrData=$corrData		\
	prior=$prior			\
	datacov=$datacov		\
	impedance=$impedance		\
	p=$p				\
	s=$s				\
	r=$r				\
	verbose=$verbose		\
	corrVP=$corrVP			\
	corrVS=$corrVS			\
	corrRHO=$corrRHO		\
	orientation=$orientation	\
	dz=$dz				\
	targetbeg=$targetbeg		\
	targetend=$targetend		\
	r1=$r1				\
	nr=$nr				\
	dr=$dr				\
	zs=$zs				\
	F1=$F1				\
	F2=$F2				\
	F3=$F3				\
	tau=$tau			\
	receiverfile=$receiverfile	\
	u1=$u1				\
	u2=$u2				\
	directwave=$directwave		\
	nu=$nu				\
	f1=$f1				\
	f2=$f2				\
	dt=$dt				\
	tmod=$tmod			\
	t1=$t1				\
	t2=$t2				\
	mutefile=$mutefile              \
	hanning=$hanning		\
	wu=$wu				\
	ww=$ww				\
	nproc=$nproc			\
	nfreqproc=$nfreqproc		\
	maxiter=$maxiter
